{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Stream Connecticut Public Radio on Alexa-enabled devices.",
          "examplePhrases": [
            "Alexa, open Connecticut Public Radio",
            "Alexa, play Connecticut Public Radio",
            "Alexa, ask Connecticut Public Radio to play"
          ],
          "keywords": [
            "audio streaming"
          ],
          "name": "Connecticut Public Radio",
          "description": "Stream Connecticut Public Radio on Alexa-enabled devices.",
          "smallIconUri": "https://api.ctpublic.org/alexa/images/alexa-small.png",
          "largeIconUri": "https://api.ctpublic.org/alexa/images/alexa-large.png"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "No special instructions",
      "category": "STREAMING_SERVICE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        },
        "interfaces": [
          {
            "type": "AUDIO_PLAYER"
          }
        ]
      }
    },
    "manifestVersion": "1.0",
    "permissions": [],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "https://www.ctpublic.org/about/privacy-policy-and-terms-of-use",
          "privacyPolicyUrl": "https://www.ctpublic.org/about/privacy-policy-and-terms-of-use"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    }
  }
}
